

// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}



model User {
  id    Int     @default(autoincrement()) @id
  email String  @unique
  firstName  String
  lastName  String
  password  String
  phoneNumber  String
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}
